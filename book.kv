        MDLabel:
            text: 'Сначала нарушьте все правила'
            theme_text_color: "Custom"
            text_color: 1, 1, 1, 1
            pos_hint: {'center_y': .96, 'center_x': .5}
            halign: 'center'
            bold: True
            font_size: 23
        MDIconButton:
            icon: 'arrow-left-circle'
            pos_hint: {'center_y': .96, 'center_x': .05}
            theme_text_color: 'Custom'
            text_color: [1, 1, 1, 1]
            on_press:
                root.manager.current = 'book'
                root.manager.transition.direction = 'right'
        BoxLayout:
            size_hint_y: .88

            on_press:
                root.manager.current = 'book'
                root.manager.transition.direction = 'right'
        BoxLayout:
            size_hint_y: .88
            pos_hint: {'center_y':.46}
            canvas.before:
                Color:
                    rgba: 0, 0, 0, 0
                Rectangle:
                    pos: self.pos
                    size: self.size
            MDCarousel:

                #image
                MDFloatLayout:
                    Image:
                        source: 'image/Бизнес/2.jpg'
                        size_hint: .6, .8
                        pos_hint: {"center_x": .5, "center_y": .5}

                #0
                MDFloatLayout:
                    MDLabel:
                        text: '10 фактов из книги «Сам себе MBA»'
                        halign: 'center'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .6}
                        font_size: 30
                        bold: True
                        padding_x: 10
                        markup: True

                #1
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]1. Ценность бизнеса[/size][/b] \n \n Автор убежден, что любое дело должно иметь практическую пользу для людей. Выбирая сферу деятельности, следует ориентироваться на человеческие потребности. Нужно выяснить, чего не хватает потребителю сегодня – и предложить ему это. Зачастую новая ценность находится в легком тюнинге уже привычных вещей. Следите за тем что, когда, где и как вы покупаете. Любые неудобства записывайте в блокнот. Через время вы найдете сферы в которых есть что улучшить чтоб сделать вас как потребителя чуть счастливей. Это значит что вы нашли новую ценность. Но будьте готовы, что это сделали не только вы.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True


                #2
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]2. Конкуренция[/size][/b] \n \n Если есть конкуренция, значит, есть спрос на товар или услугу. Вы не останетесь без целевой аудитории, клиентов, следовательно, и без прибыли. Не стоит бояться конкуренции, а напротив, нужно изучить аналогичные предложения на рынке и дать потребителю больше остальных. Предлагаемую продукцию следует предварительно «протестировать» и выявить возможные недочеты до того, как она попадет к конечному потребителю.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True


                #3
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]3. Цели маркетинга[/size][/b] \n \n Джош Кауфман убежден, что главная цель маркетинга – привлечение внимания к продукту. Сделать это можно несколькими способами, среди которых незаурядность, возбуждение любопытства, акцентирование внимания на выгодах для клиента, бесплатные предложения. Автор дает рекомендации по составлению так называемых «крючков» – коротких фраз, слоганов, привлекающих и удерживающих внимание ЦА.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True

                #4
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]4. Эффективные продажи[/size][/b] \n \n Помните о том, что главная задача любого бизнеса, ради чего собственно все и затевается — это продажи. Больше, чаще, с увеличивающимся чеком. Автор предлагает использование методик, увеличивающих продажи продукта. Они основаны на базовых психологических принципах. Например, чувство взаимности. Если вы начинаете взаимодействие с клиентом с небольшого презента, чашки кофе – это обязывает его воспользоваться вашими услугами в дальнейшем. Другой пример – перенос риска. Покупатель не желает брать на себя хлопоты. Предложение бесплатной доставки, возврат без ограничений – все это снижает покупательский барьер и увеличивает вероятность продажи.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True


                #5
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]5. Увеличение доходов[/size][/b] \n \n Есть 4 способа увеличить прибыль: увеличение количества клиентов, повышение суммы среднего чека, рост числа сделок с каждым клиентом, повышение цены. Каждому методу автор уделяет внимание и обозначает, в каких случаях целесообразно их применять.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True

                #6
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]6. Работа со страхами клиентов[/size][/b] \n \n Человек боится потерь, именно поэтому в казино при игре используются не деньги, а фишки. Страх пропадает, хотя, согласитесь, в казино люди теряют немалые суммы. Другой пример – покупка дорогого платья. Его сложно купить, если оно самое дорогое в магазине. Но если это же платье по такой же цене будет висеть с платьями высшей стоимости – вероятность его покупки увеличивается.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True
                #7
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]7. Работа над собой[/size][/b] \n \n Прайминг – самопрограммирование мозга на восприятие актуальной информации. Например, перед прочтением книги, вы заранее определяете, какая информация вас интересует, тогда в процессе чтения мозг будет отсекать лишнюю информацию. Это же правило работает и при принятии бизнес-решений. В качестве способа определения правильности своих убеждений автор предлагает заняться поиском контраргументов, что избавит от предвзятого мнения, будто ваше мнение или решение изначально верное.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True

                #8
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]8. Показатели эффективности[/size][/b] \n \n Автор предлагает анализировать систему по следующим параметрам: как быстро создается продукт, сколько людей обращает на него внимание, сколько из них его покупает, сколько времени уходит на обслуживание, какая маржа прибыли, количество возвратов или жалоб, достаточно ли вам зарабатываемых денег.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True

                #9
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]9. Улучшение системы[/size][/b] \n \n Принцип Парето: в любой системе за большую долю результата отвечает минимум параметров. Концентрируйтесь лишь на самых важных факторах, приводящих к результату.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True

                #10
                MDFloatLayout:
                    MDLabel:
                        text: '[b][size=25]10. Принципы управления[/size][/b] \n \n Нанимать на работу нужно профессионалов. Тогда даже небольшая группа быстро и качественно выполнит основной объем работы. За каждым сотрудником закрепляйте обязанности с формулировкой результата. При взаимодействии проявляйте вежливость, уважение и признательность. Ограждайте сотрудников от отвлекающих факторов и находите кратчайшие пути достижения результата. Проводите измерения, насколько быстро и хорошо выполняются поставленные задачи.'
                        halign: 'left'
                        theme_text_color: "Custom"
                        text_color: 1, 1, 1, 1
                        pos_hint: {'center_x': .5, 'center_y': .5}
                        font_size: 19
                        padding_x: 10
                        markup: True
